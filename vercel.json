{
  "runtime": "php:8.2",
  "functions": {
    "api/index.php": {
      "runtime": "php:8.2"
    },
    "index.php": {
      "runtime": "php:8.2"
    }
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/(.*)",
      "dest": "/index.php"
    }
  ]
}
